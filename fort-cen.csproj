<Project Sdk="Microsoft.NET.Sdk.Web">
  <PropertyGroup>
    <Version>3.3.0</Version>
    <AssemblyVersion>3.3.0.23306</AssemblyVersion>
    <FileVersion>3.3.0.23306</FileVersion>
    <InformationalVersion>3.3.0.23306</InformationalVersion>
  </PropertyGroup>
  <PropertyGroup>
    <TargetFrameworks>net6.0</TargetFrameworks>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <IsPackable>true</IsPackable>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(RunConfiguration)' == 'fort_cen' " />
  <ItemGroup>
    <PackageReference Include="Microsoft.Fast.Components.FluentUI" Version="3.5.0" />
  </ItemGroup>
  <ItemGroup>
        <CssFiles Include="fort_cen.styles.css"/>
    </ItemGroup>
      <ItemGroup>
    <Content Include="*.css">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>  
  </ItemGroup>
</Project>